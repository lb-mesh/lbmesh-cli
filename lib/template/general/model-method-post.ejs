
  /**
   * @method <%= methodname %> <%= modelname %>/<%= methodpath %>
   *
   **/
  <%= modelname %>.<%= methodname %> = function(formdata, cb){
      

        cb(null,formdata);
  };

  <%= modelname %>.remoteMethod('<%= methodname %>', {
        accepts: {arg: 'formdata', type: 'object', http: { source: 'body'}},
        returns: {arg: 'status', type: 'object', root: true },
        http: {path: "/<%= methodpath %>", verb: "POST"},            
  });