apps:
  - script   : ./backend/databank/server/server.js
    name     : '<%= appname %>-databank'
    instances: 1
    exec_mode: fork
    watch  : false
    env    :
      NODE_ENV: development
      MESSENGER_HOST: localhost
      MESSENGER_PORT: <%= port_messenger %>
      DB_USER:
      DB_URL:
      DB_PASS:
      DB_HOST:
      DB_NAME:
      DB_TYPE:
    env_production:
      NODE_ENV: production
      DB_USER:
      DB_URL:
      DB_PASS:
      DB_HOST:
      DB_NAME:
      DB_TYPE: