apps:
  - script   : ./backend/scheduler/server/server.js
    name     : '<%= appname %>-scheduler'
    instances: 1
    exec_mode: fork
    watch  : false
    env    :
      NODE_ENV: development
      MESSENGER_HOST: localhost
      MESSENGER_PORT: <%= port_messenger %>
      DATABANK_HOST: localhost
      DATABANK_PORT: <%= port_databank_ %>
    env_production:
      NODE_ENV: production