apps:
  - script   : ./frontend/api/server/server.js
    name     : '<%= appname %>-api'
    instances: 1
    exec_mode: fork
    watch  : false
    env    :
      NODE_ENV: development
      MESSENGER_HOST: localhost
      MESSENGER_PORT: <%= port_messenger %>
      DATABANK_HOST: localhost
      DATABANK_PORT: <%= port_databank %>
      APPNAME: <%= appname %>-api
      DB_USER:
      DB_PASS:
      DB_URL:
      DB_HOST:
      DB_NAME:
      DB_TYPE:
    env_production:
      NODE_ENV: production
      DB_USER:
      DB_PASS:
      DB_URL:
      DB_HOST:
      DB_NAME:
      DB_TYPE: