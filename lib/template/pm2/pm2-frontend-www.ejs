apps:
  - script   : ./frontend/www/server/server.js
    name     : '<%= appname %>-www'
    instances: 1
    exec_mode: fork
    watch  : false
    env    :
      NODE_ENV: development
      MESSENGER_HOST: localhost
      MESSENGER_PORT: <%= port_messenger %>
      DATABANK_HOST: localhost
      DATABANK_PORT: <%= port_databank %>
      APPNAME: "<%= appname %>-www"
      SITETITLE: "<%= appname %>-www"
      DB_USER: ""
      DB_URL: "mongodb://localhost:27017"
      DB_PASS: ""
      DB_HOST: ""
      DB_NAME: ""
      DB_TYPE: "MONGO"
    env_production:
      NODE_ENV: production
      DB_USER: ""
      DB_URL: "mongodb://localhost:27017"
      DB_PASS: ""
      DB_HOST: ""
      DB_NAME: ""
      DB_TYPE: "MONGO"