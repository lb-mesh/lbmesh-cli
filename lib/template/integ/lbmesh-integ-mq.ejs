version: "2"
services:

    mq:
      container_name: lbmesh-integ-mq
      environment:
        - LICENSE=accept
        - MQ_QMGR_NAME=QM1
        - MQ_DEV=TRUE
        - MQ_ADMIN_PASSWORD=lbmesh-integ-mq
        - MQ_APP_PASSWORD=lbmesh-mq-app
        - MQ_ENABLE_METRICS=true
      mem_reservation: 256m
      mem_limit: 1536m
      ports:
        - '9443:9443'
        - '9157:9157'
        - '1416:1414'
      image: ibmcom/mq:latest
      networks:
        - lbmesh-integ-mqadvanced

networks:
  lbmesh-integ-mqadvanced: