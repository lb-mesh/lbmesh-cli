version: "2"
services:

    mqlight:
      container_name: lbmesh-integ-mqlight
      environment:
        - LICENSE=accept
      mem_reservation: 512m
      mem_limit: 1536m
      volumes:
        - '<%= mqlight_data %>:/var/mqlight'
      ports:
        - '5672:5672'
        - '9180:9180'
      image: ibmcom/mqlight:1.0
      networks:
        - lbmesh-integ-mqlight

networks:
  lbmesh-integ-mqlight: