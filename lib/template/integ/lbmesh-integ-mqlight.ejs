version: "2"
services:

    mqlight:
      container_name: lbmesh-integ-mqlight
      environment:
        - LICENSE=accept
      mem_reservation: 256m
      mem_limit: 512m
      volumes:
        - '<%= homedir_data %>:/var/mqlight'
      ports:
        - ':5672'
        - '9180:9180'
      image: ibmcom/mqlight:1.0