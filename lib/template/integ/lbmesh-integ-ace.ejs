version: "2"
services:

    ace:
        container_name: lbmesh-integ-ace
        ports:
            - '<%= ace_port_admin %>:7600'
            - '<%= ace_port_data %>:7800'
            - '7843:7843'
        mem_reservation: 384m
        mem_limit: 1536m
        environment:
            - LICENSE=accept
            - ACE_SERVER_NAME=ACESERVER
        image: 'ibmcom/ace:latest'