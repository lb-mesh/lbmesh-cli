version: "2"
services:

    iib:
      container_name: lbmesh-integ-iib
      environment:
        - LICENSE=accept
        - NODENAME=IIBNODE
        - SERVERNAME=IIBSERVER
      mem_reservation: 256m
      mem_limit: 1024m
      volumes:
        - '<%= iib_data %>:/tmp/BARs'
      ports:
        - '7800:7800'
        - '4414:4414'
      image: ibmcom/iib:latest
      networks:
        - lbmesh-integ-iib

networks:
  lbmesh-integ-iib: