version: "2"
services:

    iib:
      container_name: lbmesh-integ-iib
      environment:
        - LICENSE=accept
        - NODENAME=IIBNODE
        - SERVERNAME=IIBSERVER
      mem_reservation: 256m
      mem_limit: 1024m
      volumes:
        - '<%= iib_data %>:/tmp/BARs'
      ports:
        - '<%= iib_port_data %>:7800'
        - '<%= iib_port_admin %>:4414'
      image: ibmcom/iib
      networks:
        - lbmesh-integ-iib

networks:
  lbmesh-integ-iib: