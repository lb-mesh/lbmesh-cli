version: "2"
services:
    acemq:
        container_name: lbmesh-integ-acemq
        ports:
            - '7600:7600'
            - '7800:7800'
            - '7843:7843'
            - '9444:9443'
            - '1414:1414'
        mem_reservation: 256m
        mem_limit: 1024m
        volumes:
          - '<%= acemq_config %>:/home/aceuser/initial-config'
        environment:
            - LICENSE=accept
            - MQ_QMGR_NAME=QMGR
            - MQ_DEV=TRUE
            - MQ_ADMIN_PASSWORD=lbmesh-integ-mq
            - ACE_SERVER_NAME=ACESERVER
        networks:
          - lbmesh-integ-acemq
        image: 'ibmcom/ace-mq:latest'

networks:
  lbmesh-integ-acemq: