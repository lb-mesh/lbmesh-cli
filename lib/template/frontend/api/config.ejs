{
    "restApiRoot": "/api",
    "host": "127.0.0.1",
    "port": <%= port_api %>,
    "remoting": {
      "context": false,
      "rest": {
        "handleErrors": false,
        "normalizeHttpPath": false,
        "xml": false
      },
      "json": {
        "strict": false,
        "limit": "100kb"
      },
      "urlencoded": {
        "extended": true,
        "limit": "100kb"
      },
      "cors": false
    }
  }